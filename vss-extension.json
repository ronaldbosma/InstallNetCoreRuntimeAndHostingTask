{
    "manifestVersion": 1,
    "id": "InstallNetCoreRuntimeAndHosting",
    "version": "0.0.0",
    "name": "Install .NET Core Runtime & Hosting Bundle",
    "publisher": "rbosma",
    "description": "Azure DevOps pipeline task that downloads and installs the latest .NET Core runtime and hosting bundle.",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "icon.png"
     },
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
       "details": {
           "path": "README.md"
       },
       "license": {
           "path": "LICENSE"
       }
   },
    "tags": [
        ".NET Core",
        "ASP.NET Core",
        "Runtime & Hosting Bundle"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/ronaldbosma/InstallNetCoreRuntimeAndHostingTask/blob/master/README.md"
        },
        "support": {
            "uri": "https://github.com/ronaldbosma/InstallNetCoreRuntimeAndHostingTask/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ronaldbosma/InstallNetCoreRuntimeAndHostingTask"
    },
    "files": [
        {
            "path": "InstallNetCoreRuntimeAndHostingTask"
        },
        {
            "path": "release-pipeline-example.png", "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "InstallNetCoreRuntimeAndHosting",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "InstallNetCoreRuntimeAndHostingTask"
            }
        }
    ]
}